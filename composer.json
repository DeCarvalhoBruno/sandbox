{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "cyrildewit/eloquent-viewable": "^3.2",
        "fideloper/proxy": "~4.0",
        "google/apiclient": "^2.2",
        "intervention/image": "^2.4",
        "laravel/framework": "5.8.*",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "~1.0",
        "laravolt/avatar": "^2.2",
        "mailgun/mailgun-php": "^2.8.1",
        "mews/purifier": "^2.1",
        "pda/pheanstalk": "^4.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.7.2",
        "ramsey/uuid": "^3.8",
        "suin/php-rss-writer": "^1.6",
        "thepixeldeveloper/sitemap": "^5.1",
        "tymon/jwt-auth": "1.0.0-rc.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "~2.3",
        "fzaninotto/faker": "~1.8",
        "laravel/dusk": "^4.0",
        "league/csv": "^9.2",
        "mockery/mockery": "~1.2",
        "nunomaduro/collision": "~2.1",
        "php-ds/php-ds": "^1.2",
        "phpunit/phpunit": "~7.5",
        "rap2hpoutre/laravel-log-viewer": "^1.0",
        "symfony/thanks": "^1.1",
        "naraki/elasticsearch": "dev-master#5448eaa as 0.1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/elasticsearch",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Naraki\\Blog\\": "naraki/blog/src/",
            "Naraki\\Rss\\": "naraki/rss/src/",
            "Naraki\\Media\\": "naraki/media/src/",
            "Naraki\\Mail\\": "naraki/mail/src/",
            "Naraki\\Sitemap\\": "naraki/sitemap/src/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar",
                "cyrildewit/eloquent-viewable"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
